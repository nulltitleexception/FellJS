extends includes/wrapper

block settings
	- var displayGithubBanner = true;
	//- - var title = "";

block head
	+css(['index','customtexture','game'])
	+js(['newuser'])

block content
	#wrapper

		#login
			form#loginInfo(action="#")
				label(for="user") Username:
				input(type="text", name="user")
				br
				label(for="pass1") Password:
				input(type="password", name="pass1")
				br
				label(for="pass2") Repeat password:
				input(type="password", name="pass2")
				br
				input(type="submit", value="Create")